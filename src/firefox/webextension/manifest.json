{
  "manifest_version": 2,
  "name": "Szentírás",
  "description": "Bibliai hivatkozások beillesztése a Szentírás.hu oldalról",
  "version": "1.3",
  "permissions": [
    "storage",
    "http://szentiras.hu/"
  ],
  "background": {
    "scripts": [ "background.js" ]
  },
  //"options_ui": {
  //  "page": "options.html"
  //},
  "content_scripts": [
    {
      "matches": [ "http://*/*", "https://*/*" ],
      "js": [ "igemutato.js", "content.js" ],
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    "igemutato.css"
  ]
}
