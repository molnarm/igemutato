{
	"manifest_version": 2,
	"name": "Szentírás",
	"description": "Bibliai hivatkozások beillesztése a Szentírás.hu oldalról",
	"version": "1.1",
	"icons": {
		"128" : "icon.png"
	},
	"permissions": [
		"storage",
	    "http://szentiras.hu/"
	],
	"options_ui":{
		"page": "options.html",
		"chrome_style": true
	},
	"options_page": "options.html",
	"content_scripts": [
		{
			"matches": [ "http://*/*", "https://*/*" ],
			"js": ["igemutato.min.js", "content.js"]
		}
	],
	"web_accessible_resources": [
        "igemutato.min.css"
    ]
}
